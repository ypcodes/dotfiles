{
   "action": {
      "default_icon": {
         "128": "icons/icon128.png",
         "16": "icons/icon16.png",
         "48": "icons/icon48.png"
      },
      "default_popup": "options.html",
      "default_title": "h264ify"
   },
   "background": {
      "service_worker": "src/service_worker.js",
      "type": "module"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "src/inject/content_script.js" ],
      "matches": [ "*://*.youtube.com/*", "*://*.youtube-nocookie.com/*", "*://*.youtu.be/*" ],
      "run_at": "document_start"
   } ],
   "default_locale": "en",
   "description": "__MSG_extensionDescription__",
   "homepage_url": "https://github.com/erkserkserks/h264ify",
   "host_permissions": [ "*://*.youtube.com/*", "*://*.youtube-nocookie.com/*", "*://*.youtu.be/*" ],
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "h264ify",
   "options_ui": {
      "page": "options.html"
   },
   "permissions": [ "scripting", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.0.1"
}
